{
  "name": "lfalck-portfolio",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "developer",
    "javascript",
    "portfolio",
    "react"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf public .cache",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.js\"",
    "serve:build": "npm run clean && npm run build && http-server ./public",
    "start": "npm run develop",
    "start:prod": "npm run build && npm run serve:build",
    "deploy:github": "npm run clean && npm run build && node ./scripts/deploy-github",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "dayjs": "^1.10.4",
    "gatsby": "^2.31.1",
    "gatsby-image": "^2.10.0",
    "gatsby-plugin-feed": "^2.12.0",
    "gatsby-plugin-google-analytics": "^2.10.0",
    "gatsby-plugin-manifest": "^2.11.0",
    "gatsby-plugin-offline": "^3.9.0",
    "gatsby-plugin-react-helmet": "^3.9.0",
    "gatsby-plugin-sharp": "^2.13.1",
    "gatsby-remark-prismjs": "^3.12.0",
    "gatsby-source-filesystem": "^2.10.0",
    "gatsby-transformer-javascript-frontmatter": "^2.9.0",
    "gatsby-transformer-remark": "^2.15.0",
    "gatsby-transformer-sharp": "^2.11.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "gatsby-plugin-sass": "^2.8.0",
    "gh-pages": "^2.2.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.14.1",
    "prettier": "^1.18.2"
  }
}
